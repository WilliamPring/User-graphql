{
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2018,
        "ecmaFeatures": {
            "modules": true
        }
    },
    "env": {
        "node": true,
        "jest": true,
        "es6": true
    },
    "rules": {
        "linebreak-style": [
            "error",
            "unix"
        ]
    }
}